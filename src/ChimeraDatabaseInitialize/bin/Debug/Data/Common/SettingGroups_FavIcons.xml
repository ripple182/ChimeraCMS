<?xml version="1.0" encoding="utf-8" ?>
<SettingGroups>
  <SettingGroup>
    <GroupKey>FavoriteIconsSettings</GroupKey>
    <UserFriendlyName>Favorite Icon Settings</UserFriendlyName>
    <Description>Settings used to setup favorite icons on different devices when your page is bookmarked.</Description>
    <ParentCategory>4</ParentCategory>
    <SettingsList>
      <Setting>
        <Key>Fav_Icon</Key>
        <UserFriendlyName>Favorite Icon</UserFriendlyName>
        <Description>32px width 32px height image used to represent your website when bookmarked in normal browsers.</Description>
        <Value></Value>
        <EntryType>4</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList>
          <SettingAttribute>
            <Key>rel</Key>
            <Value>shortcut icon</Value>
          </SettingAttribute>
        </SettingAttributeList>
      </Setting>
      <Setting>
        <Key>Apple_Fav_Icon_Small</Key>
        <UserFriendlyName>Small Apple Favorite Icon</UserFriendlyName>
        <Description>57px width 57px height image used to represent your website when bookmarked on apple devices.</Description>
        <Value></Value>
        <EntryType>4</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList>
          <SettingAttribute>
            <Key>rel</Key>
            <Value>apple-touch-icon</Value>
          </SettingAttribute>
        </SettingAttributeList>
      </Setting>
      <Setting>
        <Key>Apple_Fav_Icon_Medium</Key>
        <UserFriendlyName>Medium Apple Favorite Icon</UserFriendlyName>
        <Description>72px width 72px height image used to represent your website when bookmarked on apple devices.</Description>
        <Value></Value>
        <EntryType>4</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList>
          <SettingAttribute>
            <Key>rel</Key>
            <Value>apple-touch-icon</Value>
          </SettingAttribute>
          <SettingAttribute>
            <Key>sizes</Key>
            <Value>72x72</Value>
          </SettingAttribute>
        </SettingAttributeList>
      </Setting>
      <Setting>
        <Key>Apple_Fav_Icon_Large</Key>
        <UserFriendlyName>Large Apple Favorite Icon</UserFriendlyName>
        <Description>114px width 114px height image used to represent your website when bookmarked on apple devices.</Description>
        <Value></Value>
        <EntryType>4</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList>
          <SettingAttribute>
            <Key>rel</Key>
            <Value>apple-touch-icon</Value>
          </SettingAttribute>
          <SettingAttribute>
            <Key>sizes</Key>
            <Value>114x114</Value>
          </SettingAttribute>
        </SettingAttributeList>
      </Setting>
    </SettingsList>
  </SettingGroup>
</SettingGroups>