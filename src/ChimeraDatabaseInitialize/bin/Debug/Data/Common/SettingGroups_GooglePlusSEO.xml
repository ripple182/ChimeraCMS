        <Value></Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList>
          <SettingAttribute>
            <Key>SettingAttributeValueKey</Key>
            <Value>href</Value>
