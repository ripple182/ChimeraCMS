<?xml version="1.0" encoding="utf-8" ?>
<SettingGroups>
  <SettingGroup>
    <GroupKey>TwitterSEOSettings</GroupKey>
    <UserFriendlyName>Twitter SEO Settings</UserFriendlyName>
    <Description>Settings used to identify your website whenever a customer posts a link to your website on Twitter.</Description>
    <ParentCategory>1</ParentCategory>
    <SettingsList>
      <Setting>
        <Key>Card</Key>
        <UserFriendlyName>Type</UserFriendlyName>
        <Description>This value should not change, should default to "summary" to tell Twitter what the content should look like when posted on Twitter.</Description>
        <Value>summary</Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList>
          <SettingAttribute>
            <Key>SettingAttributeValueKey</Key>
            <Value>content</Value>
          </SettingAttribute>
          <SettingAttribute>
            <Key>name</Key>
            <Value>twitter:card</Value>
          </SettingAttribute>
        </SettingAttributeList>
      </Setting>
      <Setting>
