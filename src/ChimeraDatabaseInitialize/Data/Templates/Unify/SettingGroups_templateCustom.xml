<?xml version="1.0" encoding="utf-8" ?>
<SettingGroups>
  <SettingGroup>
    <GroupKey>templateCustomSettings</GroupKey>
    <UserFriendlyName>Website Custom Settings</UserFriendlyName>
    <Description>Custom website settings include things like the Website Title, store address, email address, theme colors, etc.</Description>
    <ParentCategory>3</ParentCategory>
    <SettingsList>
      <Setting>
        <Key>WebsiteTitle</Key>
        <UserFriendlyName>Website Title</UserFriendlyName>
        <Description>The title used on every page to identify the website.</Description>
        <Value>Website Title</Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>ThemeColor</Key>
        <UserFriendlyName>Theme Color</UserFriendlyName>
        <Description>The overall theme color used for your website, limited selection.</Description>
        <Value>color-default</Value>
        <EntryType>2</EntryType>
        <DataEntryStaticPropertyKey>TemplateThemeColors</DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>ThemeHeaderStyle</Key>
        <UserFriendlyName>Theme Header Style</UserFriendlyName>
        <Description>The style used for the header of the website.</Description>
        <Value>header1</Value>
        <EntryType>2</EntryType>
        <DataEntryStaticPropertyKey>TemplateThemeHeaderStyles</DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>ThemeHeaderColor</Key>
        <UserFriendlyName>ThemeHeaderColor</UserFriendlyName>
        <Description>The color used for the header of the website.</Description>
        <Value>header1-blue</Value>
        <EntryType>2</EntryType>
        <DataEntryStaticPropertyKey>TemplateThemeHeaderColors</DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>PhoneNumber</Key>
        <UserFriendlyName>Phone Number</UserFriendlyName>
        <Description>The phone number a visiting user can use to contact you, leave field empty if you don't wish to show a contact phone number.</Description>
        <Value>555-555-5555</Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>EmailAddress</Key>
        <UserFriendlyName>Email Address</UserFriendlyName>
        <Description>The email address a visiting user can use to contact you, leave field empty if you don't wish to show a contact email address.</Description>
        <Value>something@gmail.com</Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>PhysicalAddress</Key>
        <UserFriendlyName>Physical Address</UserFriendlyName>
        <Description>The physical address a visiting user can use to contact you, leave field empty if you don't wish to show a contact physical address.</Description>
        <Value>12, Srtington Street, NY, USA</Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>FooterAboutUsHeader</Key>
        <UserFriendlyName>Footer About Us Header</UserFriendlyName>
        <Description>The header text of the first content block in the footer, leave empty to not show this section at all.</Description>
        <Value>About Us</Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>FooterAboutUsText</Key>
        <UserFriendlyName>Footer About Us Text</UserFriendlyName>
        <Description>The text of the first content block in the footer, leave empty to not show this section at all.</Description>
        <Value>This is a description about our business</Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>FooterContactUsHeader</Key>
        <UserFriendlyName>Footer Contact Us Header</UserFriendlyName>
        <Description>The header text of the second content block in the footer, leave empty to not show this section at all.</Description>
        <Value>Contact Us</Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>FooterStayConnectedHeader</Key>
        <UserFriendlyName>Footer Stay Connected Header</UserFriendlyName>
        <Description>The header text of the third content block in the footer, leave empty to not show this section at all.</Description>
        <Value>Stay Connected</Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>CopyrightBusinessName</Key>
        <UserFriendlyName>Copyright Business Name</UserFriendlyName>
        <Description>The copyright business name shown at the bottom of the website, leave empty to not show copyright info.</Description>
        <Value>Business LLC</Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>PrivacyPolicyUrl</Key>
        <UserFriendlyName>Privacy Policy Url</UserFriendlyName>
        <Description>The URL to your privacy policy, leave empty to not show this link.</Description>
        <Value>http://localhost/ChimeraWebsite/PrivacyPolicy</Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>TermsAndConditionsUrl</Key>
        <UserFriendlyName>Terms And Conditions Url</UserFriendlyName>
        <Description>The URL to your Terms And Conditions, leave empty to not show this link.</Description>
        <Value>http://localhost/ChimeraWebsite/TermsAndConditions</Value>
        <EntryType>1</EntryType>
        <DataEntryStaticPropertyKey></DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
      <Setting>
        <Key>PageNotFoundPage</Key>
        <UserFriendlyName>Page Not Found Page</UserFriendlyName>
        <Description>The page used whenever a user is directed to a page that does not exit or is no longer publised.</Description>
        <Value>BasicView</Value>
        <EntryType>2</EntryType>
        <DataEntryStaticPropertyKey>PageNotFoundPages</DataEntryStaticPropertyKey>
        <SettingAttributeList></SettingAttributeList>
      </Setting>
    </SettingsList>
  </SettingGroup>
</SettingGroups>